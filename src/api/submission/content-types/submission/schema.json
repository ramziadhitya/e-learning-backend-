{
  "kind": "collectionType",
  "collectionName": "submissions",
  "info": {
    "singularName": "submission",
    "pluralName": "submissions",
    "displayName": "submission"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "assignment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::assignment.assignment"
    },
    "file": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "score": {
      "type": "decimal"
    },
    "students": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
