{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "category": {
      "type": "text"
    },
    "rating": {
      "type": "biginteger"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "studentsCount": {
      "type": "biginteger"
    },
    "instructor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "price": {
      "type": "biginteger"
    },
    "language": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "level": {
      "type": "enumeration",
      "enum": [
        "Beginner",
        "Intermediate",
        "Hard",
        "All Level"
      ]
    },
    "certification": {
      "type": "boolean"
    },
    "reviews": {
      "displayName": "reviews",
      "type": "component",
      "repeatable": true,
      "component": "shared.reviews"
    },
    "video": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "quizzes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quiz.quiz",
      "mappedBy": "course"
    },
    "description": {
      "type": "blocks"
    }
  }
}
